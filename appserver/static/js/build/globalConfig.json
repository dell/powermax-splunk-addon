                        "label": "Status"
                    }
                ],
                "moreInfo": [
                    {
                        "field": "name",
                        "label": "Name"
                    },
                    {
                        "field": "interval",
                        "label": "Interval"
                    },
                    {
                        "field": "index",
                        "label": "Index"
                    },
                    {
                        "field": "disabled",
                        "label": "Status"
                    },
                    {
                        "field": "u4v_ip_address",
                        "label": "Unisphere IP Address"
                    },
                    {
                        "field": "u4v_port",
                        "label": "Unisphere Port"
                    },
                    {
                        "field": "u4v_username",
                        "label": "Unisphere Username"
                    },
                    {
                        "field": "u4v_password",
                        "label": "Unisphere Password"
                    },
                    {
                        "field": "u4v_vmax_id",
                        "label": "VMAX Numerical ID"
                    },
                    {
                        "field": "enable_ssl",
                        "label": "Enable SSL"
                    },
                    {
                        "field": "ssl_cert_location",
                        "label": "SSL Cert Location"
                    },
                    {
                        "field": "rest_request_timeout",
                        "label": "REST Request Timeout (s)"
                    },
                    {
                        "field": "select_array",
                        "label": "Array"
                    },
                    {
                        "field": "select_alerts",
                        "label": "Alerts"
                    },
                    {
                        "field": "select_vmax_only_metrics",
                        "label": "Collect VMAX only metrics"
                    },
                    {
                        "field": "select_srp",
                        "label": "Storage Resource Pool"
                    },
                    {
                        "field": "select_sg",
                        "label": "Storage Group"
                    },
                    {
                        "field": "select_director",
                        "label": "Director"
                    },
                    {
                        "field": "select_port",
                        "label": "Port"
                    },
                    {
                        "field": "select_pg",
                        "label": "Port Group"
                    },
                    {
                        "field": "select_host",
                        "label": "Checkbox"
                    },
                    {
                        "field": "select_initiator",
                        "label": "Initiator"
                    },
                    {
                        "field": "select_wlp",
                        "label": "Workload Planner"
                    }
                ],
                "actions": [
                    "edit",
                    "enable",
                    "delete",
                    "clone"
                ]
            },
            "services": [
                {
                    "name": "inputs",
                    "title": "Dell EMC VMAX",
                    "entity": [
                        {
                            "field": "name",
                            "label": "Name",
                            "type": "text",
                            "help": "Enter a unique name for the data input",
                            "required": true,
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^[a-zA-Z]\\w*$",
                                    "errorMsg": "Input Name must start with a letter and followed by alphabetic letters, digits or underscores."
                                },
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 100,
                                    "errorMsg": "Length of input name should be between 1 and 100"
                                }
                            ]
                        },
                        {
                            "field": "interval",
                            "label": "Interval",
                            "type": "text",
                            "required": true,
                            "help": "Time interval of input in seconds.",
                            "validators": [
                                {
                                    "type": "regex",
                                    "pattern": "^\\-[1-9]\\d*$|^\\d*$",
                                    "errorMsg": "Interval must be an integer."
                                }
                            ]
                        },
                        {
                            "field": "index",
                            "label": "Index",
                            "type": "singleSelect",
                            "defaultValue": "default",
                            "options": {
                                "endpointUrl": "data/indexes",
                                "blackList": "^_.*$",
                                "createSearchChoice": true
                            },
                            "required": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 1,
                                    "maxLength": 80,
                                    "errorMsg": "Length of index name should be between 1 and 80."
                                }
                            ]
                        },
                        {
                            "field": "u4v_ip_address",
                            "label": "Unisphere IP Address",
                            "help": "",
                            "required": true,
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "u4v_port",
                            "label": "Unisphere Port",
                            "help": "Unisphere default port is 8443",
                            "required": true,
                            "type": "text",
                            "defaultValue": "8443",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "8443"
                            }
                        },
                        {
                            "field": "u4v_username",
                            "label": "Unisphere Username",
                            "help": "",
                            "required": true,
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "u4v_password",
                            "label": "Unisphere Password",
                            "help": "",
                            "required": true,
                            "type": "text",
                            "encrypted": true,
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of password is 8192"
                                }
                            ]
                        },
                        {
                            "field": "u4v_vmax_id",
                            "label": "VMAX Numerical ID",
                            "help": "This is the 12-digit array ID",
                            "required": true,
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "enable_ssl",
                            "label": "Enable SSL",
                            "help": "Enable SSL communication between Splunk and Unisphere",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "ssl_cert_location",
                            "label": "SSL Cert Location",
                            "help": "(Optional) Leave this blank if the Unisphere SSL certificate is loaded into the system, otherwise you can specify the direct path to the .pem certificate",
                            "required": false,
                            "type": "text",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ],
                            "options": {
                                "placeholder": "/path/to/cert"
                            }
                        },
                        {
                            "field": "rest_request_timeout",
                            "label": "REST Request Timeout (s)",
                            "help": "(Optional) Default REST request timeout value, default is 60 seconds, for more information please consult the TA user guide",
                            "required": false,
                            "type": "text",
                            "defaultValue": "60",
                            "validators": [
                                {
                                    "type": "string",
                                    "minLength": 0,
                                    "maxLength": 8192,
                                    "errorMsg": "Max length of text input is 8192"
                                }
                            ]
                        },
                        {
                            "field": "select_array",
                            "label": "Array",
                            "help": "Collect Array level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_alerts",
                            "label": "Alerts",
                            "help": "Collect system & array alerts",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_vmax_only_metrics",
                            "label": "Collect VMAX only metrics",
                            "help": "Select this option to collect only those alerts which relate to the specified VMAX ID, for more information please consult the TA user guide.",
                            "required": false,
                            "type": "checkbox"
                        },
                        {
                            "field": "select_srp",
                            "label": "Storage Resource Pool",
                            "help": "Collect SRP level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_sg",
                            "label": "Storage Group",
                            "help": "Collect SG level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_director",
                            "label": "Director",
                            "help": "Collect Director level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_port",
                            "label": "Port",
                            "help": "Collect Port level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_pg",
                            "label": "Port Group",
                            "help": " Collect PG level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_host",
                            "label": "Checkbox",
                            "help": "Collect Host level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_initiator",
                            "label": "Initiator",
                            "help": "Collect Initiator level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        },
                        {
                            "field": "select_wlp",
                            "label": "Workload Planner",
                            "help": "Collect WLP level information",
                            "required": false,
                            "type": "checkbox",
                            "defaultValue": true
                        }
                    ]
                }
            ]
        }
    }
}